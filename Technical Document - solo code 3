/* body styling: */
body {
  min-width: 300px;
  color: black;
  background-color: white;
  font-family: 'Open Sans', Helvetica;
  line-height: 1.5;
}

/* navbar styling: */
#navbar {
  position: fixed;
  top: 0px;
  left: 0px;
  min-width: 300px;
  height: 100%;
  border-right: dashed;
  border-color: black}
header {
  color: black;
  margin: 5px;
  text-align: center;
  font-size: 2em;
}
#navbar ul {
  height: 85%;
  padding: 0;
  overflow: auto;
}
#navbar li {
  position: relative;
  width: 100%;
  border-top: 2px dashed;
  color: black;
  list-style: none;
}
#navbar a {
  display: block;
  padding: 10px 30px;
  color: black;
  text-decoration: none;
  cursor: pointer;
}
#navbar a:hover {color: blue;}

/* main-doc styling: */
#main-doc header {
  margin: 0px;
  text-align: left;
}
#main-doc {
  position: absolute;
  padding: 20px;
  margin-left: 310px;
  margin-bottom: 110px;
}
section article {
  color: black;
  margin: 15px;
  font-size: 0.9em;
}
section li {
  margin: 15px 0px 0px 15px;
}
code {
  display: block;
  position: relative;
  padding: 15px;
  margin: 10px;
  border-radius: 10px;
  text-align: left;
  background-color: gray;
  color: white;
  line-height: 2;
}
/* Mobile phone styling: Landscape: */
